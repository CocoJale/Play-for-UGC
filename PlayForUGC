while true do wait()
game.Players.LocalPlayer.Character.Humanoid.Jump = true
end

while true do wait(1)
local args = {
    [1] = 1500
}
game:GetService("ReplicatedStorage").Events.ForServer.UpdQuest4:FireServer(unpack(args))
end

while true do wait(1)
local args = {
    [1] = "Freeze",
    [2] = 500
}
game:GetService("ReplicatedStorage").Events.ForServer.TaskProgress:FireServer(unpack(args))
end

while true do wait(1)
local args = {
    [1] = game:GetService("Players").LocalPlayer.PlayerGui.Main.Menus.SpinFrame.SpinWheel
}
game:GetService("ReplicatedStorage").Events.ForServer.AttemptSpin:FireServer(unpack(args))
end

while true do wait(1)
local args = {
    [1] = "Click",
    [2] = 1500
}
game:GetService("ReplicatedStorage").Events.ForServer.TaskProgress:FireServer(unpack(args))
end
